<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for extra_param do |f| %>
      
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :formula_id %>
      <%= f.text_field :formula_id, readonly: :true %>

      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>


      <%= f.label :weight %>
      <%= f.number_field :weight, class: 'form-control' %>

      <%= f.submit button_text, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
